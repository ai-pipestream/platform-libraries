{
  "name": "@ai-pipestream/grpc-stubs",
  "version": "0.1.2",
  "description": "Generated gRPC stubs for Pipestream AI platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "generate": "buf generate ../grpc-stubs/src/main/proto && tsc",
    "build": "npm run clean && npm run generate",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "grpc",
    "protobuf",
    "typescript",
    "pipestream",
    "ai"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-pipestream/platform-libraries.git",
    "directory": "grpc/node"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.56.0",
    "@bufbuild/protoc-gen-es": "latest",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@bufbuild/protobuf": "latest",
    "@connectrpc/connect": "latest",
    "@connectrpc/connect-web": "latest"
  },
  "engines": {
    "node": ">=22.0.0 <23",
    "pnpm": ">=10 <11"
  }
}
